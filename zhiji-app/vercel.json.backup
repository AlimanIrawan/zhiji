{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    },
    {
      "src": "api/garmin.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/garmin",
      "dest": "/api/garmin.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}